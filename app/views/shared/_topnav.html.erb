<section>
    <% if user_signed_in? %>
        <div id="logout"><%= link_to 'Logout', destroy_user_session_path, method: "delete" %></div>
        <div id="username"><%= current_user.email %></div>
    <% else %>
        <p><%= link_to 'Login', new_user_session_path %>
|
        <%= link_to 'Sign Up', new_user_registration_path %>
        </p>
    <% end %>
</section>